<?xml version="1.0" encoding="utf-8"?>
        <!--银行系统-->
<bank version="1.3">
<!--系统配置-->
<configure>
    <!--dao接口配置-->
    <dao default="filedata">
        <!--文件存储-->
        <dataSrc type="filedata" object="com.cx.bank.dao.FileDaoImpl">
            <!--文件的存储路径-->
            <property name="file_url" value="filedata//register"/>
            <!--系统默认存储模板-->
            <property name="template" value="filedata//template.properyties"/>
        </dataSrc>
    </dao>
</configure>
</bank>